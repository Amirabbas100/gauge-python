{
  "id": "python",
  "version": "0.0.3",
  "description": "Python support for gauge",
  "run": {
    "windows": [
       "start.bat", "--start"
    ],
    "linux": [
      "./start.py", "--start"
    ],
    "darwin": [
      "./start.py", "--start"
    ]
  },
  "init": {
    "windows": [
       "start.bat", "--init"
    ],
    "linux": [
      "./start.py", "--init"
    ],
    "darwin": [
      "./start.py", "--init"
    ]
  },
  "gaugeVersionSupport": {
    "minimum": "0.3.0",
    "maximum": ""
  }
}
